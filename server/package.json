{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --files src/index.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only -r tsconfig-paths/register --files src/index.ts",
    "start": "node dist/index.js",
    "build": "rimraf dist && tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "seed": "ts-node -r tsconfig-paths/register src/seeds/seedDatabase.ts",
    "seed:dev": "ts-node-dev -r tsconfig-paths/register src/seeds/seedDatabase.ts",
    "seed:posts": "ts-node -r tsconfig-paths/register src/seeds/addMorePosts.ts",
    "seed:hipcamp": "ts-node -r tsconfig-paths/register src/seeds/seedHipcamp.ts",
    "seed:availability": "ts-node -r tsconfig-paths/register src/seeds/seedAvailability.ts",
    "migrate:property-site": "ts-node -r tsconfig-paths/register src/scripts/migrate-to-property-site.ts",
    "migrate:rollback": "ts-node -r tsconfig-paths/register src/scripts/rollback-property-site.ts",
    "validate:migration": "ts-node -r tsconfig-paths/register src/scripts/validate-migration.ts",
    "migrate:concurrent-bookings": "ts-node -r tsconfig-paths/register src/scripts/migrate-concurrent-bookings.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^5.3.3",
    "@payos/node": "^2.0.3",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.7.0",
    "colors": "^1.4.0",
    "cookie": "^1.0.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.2.0",
    "multer": "^2.0.2",
    "ngrok": "^5.0.0-beta.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "redis": "^5.9.0",
    "resend": "^3.2.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.11",
    "@types/nodemailer": "^7.0.1",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.2",
    "mongodb-memory-server": "^10.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.4.5"
  }
}
